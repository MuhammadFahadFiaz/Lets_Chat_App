<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.fahad.letschat.MainActivity">
    <include layout="@layout/activity_menu_bar"
        android:id="@+id/main_menu_bar"></include>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome janu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/main_id"
        android:textSize="20sp"
        android:layout_centerInParent="true"/>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/usr_id"
        android:scaleType="fitXY"
        android:layout_below="@id/main_id"
        android:layout_marginTop="10dp"
        android:layout_centerInParent="true"
        />
</RelativeLayout>
